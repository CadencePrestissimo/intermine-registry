    <section id="registry-body" class = 'pt-50'>
    	<div class="container pt-20">
        <header>
          <div class='align-center'>
            <div class='col-md-offset-1 row'>
              <div class='col-md-5'>
                <img src="http://intermine.org/assets/img/logo.png">
              </div>
              <div class='col-md-7 pt-5'>
                <a class="github-button" href="https://github.com/intermine/intermine-registry" data-size="large" aria-label="Intermine-Registry on GitHub">Source</a>
