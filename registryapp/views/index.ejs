        <div class='pt-50'>
        </div>
    		<div class="row">
      		<div class="col-md-offset-1 col-md-10 curso-desc">
            Intermine Registry is a web application that allow users to view and search basic information about existing InterMine instances. This information includes instances names, URLs, versions, neighbours and descriptions.
      		</div>
    		</div>
    		<div class="row mt-xs-20 pt-20 mt-md-20 mt-lg-20 pb-50">
    			<div class="col-lg-10 col-lg-offset-1">
            <div class="row">
              <div class="col-md-2">
                <button class="btn"> Add Instance </button>
              </div>
              <div class="col-md-7">
                <div class="inner-addon left-addon">
                    <i class="glyphicon glyphicon-search"></i>
                    <input type="text" class="form-control" />
                </div>
              </div>
              <div class="col-md-3">
                <!-- Pills -->
                <ul class="nav nav-pills navbar-right" role="tablist" id = 'intances-tabs'>
                  <li role="presentation" class="active"><a href="#instances-list" class="glyphicon glyphicon-list" aria-controls="list" role="tab" data-toggle="tab"></a></li>
                  <li role="presentation"><a href="#instances-grid" class="glyphicon glyphicon-th" aria-controls="grid" role="tab" data-toggle="tab"></a></li>
                  <li role="presentation"><a href="#instances-world" class="glyphicon glyphicon-globe" aria-controls="world" role="tab" data-toggle="tab"></a></li>
                </ul>
              </div>
            </div>

    				<!-- List View -->
            <div class='pt-20'> </div>
    				<div class="tab-content mt-lg-20">
    					<div role="tabpanel" class="tab-pane fade in active" id="instances-list">
                <table style="width:100%" class="table table-hover table-responsive">
                  <thead>
                    <tr>
                      <th class="col-md-1"></th>
                      <th class="col-md-2">Name</th>
                      <th class="col-md-8">Description</th>
                      <th class="col-md-1">Version</th>
                    </tr>
                  </thead>
                  <tbody id="list-table-body">

                  </tbody>
                </table>

                <div id="mine-modals" class="modal fade" tabindex="-1" role="dialog">
                  <div class="modal-dialog">
                  	<div class="modal-content">
                  		<div class="modal-header">
                  		<button type="button" class="close closeb" data-dismiss="modal">Ã—</button>
                  			<h3 id="modal-mine-title">Modal header</h3>
                  	  </div>
                    	<div class="modal-body" id="mine-modal-body">


                    	</div>
                    	<div class="modal-footer">
                        <button class="btn btn-primary">Update</button>
                        <button class="btn btn-danger">Delete</button>
                    		<button class="btn closeb" data-dismiss="modal">Close</button>
                    	</div>
                  	</div>
                  </div>
                </div>
    					</div>

    				  <!-- Grid View -->
  						<div role="tabpanel" class="tab-pane fade" id="instances-grid">
                <div class="container">
            			<!-- Codrops top bar -->

            			<div class="main">
            				<ul id="og-grid" class="og-grid">

            				</ul>

            			</div>
            		</div><!-- /container -->
  						</div>


  						<div role="tabpanel" class="tab-pane fade" id="instances-world">
                <div id="map" class="align-center"></div>
  						</div>

    				</div>
    			</div>
    		</div>
      </div>
